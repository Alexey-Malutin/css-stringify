{
  "stylesheet": {
    "rules": [
      {
        "media": "screen, projection",
        "rules": [
          {
            "selector": "html",
            "declarations": [
              {
                "property": "background",
                "value": "#fffef0"
              },
              {
                "property": "color",
                "value": "#300"
              }
            ]
          },
          {
            "selector": "body",
            "declarations": [
              {
                "property": "max-width",
                "value": "35em"
              },
              {
                "property": "margin",
                "value": "0 auto"
              }
            ]
          }
        ]
      },
      {
        "media": "print",
        "rules": [
          {
            "selector": "html",
            "declarations": [
              {
                "property": "background",
                "value": "#fff"
              },
              {
                "property": "color",
                "value": "#000"
              }
            ]
          },
          {
            "selector": "body",
            "declarations": [
              {
                "property": "padding",
                "value": "1in"
              },
              {
                "property": "border",
                "value": "0.5pt solid #666"
              }
            ]
          }
        ]
      }
    ]
  }
}